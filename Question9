class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        """Covert given integer to string and check from last digit is same as first digit and so on"""
        stringform=str(x)
        length=len(stringform)-1
        for x in range(length):
            if stringform[x]== stringform[length-x]:
                pass
            else:
                return False
        return True
